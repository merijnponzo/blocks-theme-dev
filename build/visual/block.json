{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "ponzoblocks/visual",
  "version": "0.1.0",
  "title": "Visual",
  "category": "ponzoblocks",
  "icon": "flag",
  "description": "A visual block",
  "attributes": {
    "blockId": {
      "type": "string",
      "default": ""
    },
    "media": {
      "type": "object",
      "default": {
        "id": 0,
        "sizes": {},
        "url": "/app/uploads/placeholder.jpg",
        "alt": "",
        "type": "",
        "width": 0,
        "height": 0
      }
    },
    "imageProps": {
      "type": "object",
      "default": {
        "imageFit": "contain",
        "imagePosition": "center",
        "aspectRatio": "square",
        "imageEffect": "",
        "imageFilter": "",
        "imageScale": ""
      }
    },
    "imageClass": {
      "type": "string",
      "default": ""
    },
    "customStyle": {
      "type": "array",
      "default": []
    },
    "customClass": {
      "type": "string",
      "default": ""
    },
    "isBanner": {
      "type": "boolean",
      "default": false
    }
  },
  "supports": {
    "html": false,
    "color": {
      "gradients": true,
      "link": true,
      "background": true,
      "text": true
    },
    "dimensions": {
      "minHeight": true
    },
    "spacing": {
      "margin": true,
      "padding": true,
      "blockGap": false
    }
  },
  "textdomain": "ponzoblocks",
  "editorScript": "file:./index.js"
}