{
  "$schema": "https://schemas.wp.org/trunk/block.json",
  "apiVersion": 2,
  "name": "ponzoblocks/heading",
  "version": "0.1.0",
  "title": "Heading",
  "category": "ponzoblocks",
  "icon": "flag",
  "description": "heading block",
  "attributes": {
    "content": {
      "type": "string",
      "default": "Heading"
    },
    "textAlign": {
      "type": "string",
      "default": "text-left"
    },
    "textAlignDesktop": {
      "type": "string",
      "default": ""
    },
    "contentTag": {
      "type": "string",
      "default": "p"
    },
    "customStyle": {
      "type": "array",
      "default": [
        {
          "responsive": "xs",
          "modifier": "width",
          "modifierPrefix": "w-",
          "unit": "448",
          "setting": "dimension",
          "id": "xs:w-",
          "tw": "w-448"
        }
      ]
    },
    "customClass": {
      "type": "string",
      "default": "w-448"
    }
  },
  "supports": {
    "border": {
      "color": true,
      "radius": true,
      "style": true,
      "width": true
    },
    "html": false,
    "color": {
      "gradients": true,
      "link": true,
      "background": true,
      "text": true
    },
    "dimensions": {
      "minHeight": true
    },
    "spacing": {
      "margin": true,
      "padding": true,
      "blockGap": true
    },
    "typography": {
      "fontSize": true
    },
    "align": true
  },
  "textdomain": "ponzoblocks",
  "editorScript": "file:./index.js"
}
